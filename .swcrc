{
    "jsc": {
         "parser": {
         "syntax": "typescript",
         "tsx": false,
         "dynamicImport": false,
         "decorators": true
         },
        "transform": {
            "legacyDecorator": true,
            "decoratorMetadata": true
        },
        "target": "es2022",
        "loose": false,
        "externalHelpers": false,
        "keepClassNames": false,
        "minify": {
            "compress" : false,
            "mangle" : false
        },
        "baseUrl": "src",
        "paths": {}   
    },
    "module": {
        "type": "commonjs"
    }
}